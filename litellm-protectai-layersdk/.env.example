# LiteLLM API Keys
# Get your API key from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Layer SDK Configuration
# Replace with your actual Layer instance details
LAYER_BASE_URL=https://layer.demo02.protectai.cloud/
LAYER_APPLICATION_ID=your_application_id_here
LAYER_ENVIRONMENT=demo02
LAYER_FIREWALL_BASE_URL= https://layer-firewall.demo02.protectai.cloud

# Layer OIDC Authentication
# Get these from your Layer admin console
LAYER_OIDC_TOKEN_URL=https://auth.protectai.cloud/realms/demo02/protocol/openid-connect/token
LAYER_OIDC_CLIENT_ID=demo02/layer-sdk
LAYER_OIDC_CLIENT_SECRET=your_layer_client_secret_here

# LiteLLM Proxy Settings
# Change this to a secure random key for production
LITELLM_MASTER_KEY=sk-1234
